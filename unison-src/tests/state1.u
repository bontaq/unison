--State1 ability
ability State se2 where
  put : ∀ se . se -> {State se} ()
  get : ∀ se . () -> {State se} se

-- state : ∀ s a . s -> Request (State s) a -> (s, a)
state woot eff = case eff of
  { State.put snew -> k } -> handle (state snew) with k ()
  { State.get () -> k } -> handle state woot with k woot
  { a } -> (woot, a)

blah : ∀ s a . s -> Request (State s) a -> (s, a)
blah = state

